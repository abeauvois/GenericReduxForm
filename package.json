{
  "name": "generic-redux-form",
  "version": "1.4.0",
  "description": "Create multi-platform redux forms in a few lines of code, using native-base (web+mobile) or material-ui (web)",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/generic-redux-form.js",
    "clean": "rimraf dist lib",
    "prepublish": "npm run test && npm run clean && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abeauvois/generic-redux-form.git"
  },
  "keywords": [
    "react",
    "react-native",
    "ios",
    "android",
    "redux",
    "form",
    "redux-form",
    "generic-redux-form",
    "material-ui",
    "native-base"
  ],
  "author": "Alexandre Beauvois <abeauvois@gmail.com> (https://fr.linkedin.com/in/abeauvois)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abeauvois/generic-redux-form/issues"
  },
  "homepage": "https://github.com/abeauvois/generic-redux-form#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015-no-commonjs": "0.0.2",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "cross-env": "^3.1.3",
    "eventsource-polyfill": "^0.9.6",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "json-loader": "^0.5.4",
    "react-addons-test-utils": "^15.4.1",
    "rimraf": "^2.5.4",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3"
  }
}
